{
  "manifest_version": 2,
  "name": "Worm tags",
  "version": "0.0.1",
  "author": "matsMaker",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["main.js"]
  }],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "web_accessible_resources": [
    "img/demonscrestsheet7.gif"
  ]
}
